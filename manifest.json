{
    "name" : "Youtube-voice extension",
    "description": "Extension for youtube voice change",
    "version":"0.1.0",
    "manifest_version":2,
    "icons":{
        "16": "./Speaker_Icon_clip_art_small.png",
        "32": "./Speaker_Icon_clip_art_medium.png",
        "48": "./Speaker_Icon_clip_art_hight.png"
    },
    "background" :{
        "scripts" :["./background.js"]
    },
    
    "options_page":"./options.html",
    
    "browser_action" :{
        "default_popup" :"popup.html"

    },

    "permissions":[
        "tabs", 
        "https://www.youtube.com/*"
    ]
   
    

}
